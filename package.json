  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "cross-var": "^1.1.0",
    "css-loader": "^6.8.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^2.7.6",
    "minimist": "^1.2.5",
    "node-stream-zip": "^1.15.0",
    "prettier": "^2.4.1",
